#include <stdio.h>
#include <stdbool.h>

void backtrack(int idx, int n, int nums[], int perm[], bool used[]) {
    
    if (idx == n) {
        for (int i = 0; i < n; i++){
            printf("%d ", perm[i]);
        }
        printf("\n");
        return;
    }

    for (int i = 0; i < n; i++) {
        if (!used[i]) {
            used[i] = true;
            perm[idx] = nums[i];

            backtrack(idx + 1, n, nums, perm, used);

            used[i] = false;
        }
    }
}

int main() {
    int n;
    scanf("%d", &n);

    int nums[n];
    int perm[n];
    bool used[n];

    for (int i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
        used[i] = false;
    }

    backtrack(0, n, nums, perm, used);
    return 0;
} 
